<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  <bpmn:process id="thunderstruck-process" name="ThunderStruck Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Process Started">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Activity_DMN" />
    
    <bpmn:businessRuleTask id="Activity_DMN" name="Avaliar Prioridade e SLA">
      <bpmn:extensionElements>
        <zeebe:calledDecision decisionId="SlaPriorityDecision" resultVariable="sla_result" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_To_Main</bpmn:outgoing>
    </bpmn:businessRuleTask>
    
    <bpmn:sequenceFlow id="Flow_To_Main" sourceRef="Activity_DMN" targetRef="Activity_Main" />
    
    <bpmn:serviceTask id="Activity_Main" name="Executar Operação Principal">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="main-operation" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_To_Main</bpmn:incoming>
      <bpmn:outgoing>Flow_End</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:boundaryEvent id="Event_SLA_Timer" name="SLA Expirado" attachedToRef="Activity_Main">
      <bpmn:outgoing>Flow_Escalation</bpmn:outgoing>
      <bpmn:timerEventDefinition id="TimerEventDefinition_1">
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">="PT" + string(sla_result.sla_horas) + "H"</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:boundaryEvent>
    
    <bpmn:sequenceFlow id="Flow_Escalation" sourceRef="Event_SLA_Timer" targetRef="Activity_Escalation" />
    
    <bpmn:serviceTask id="Activity_Escalation" name="Escalar Operação">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="escalation-handler" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Escalation</bpmn:incoming>
      <bpmn:outgoing>Flow_End_Escalation</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:endEvent id="EndEvent_1" name="Processo Finalizado">
      <bpmn:incoming>Flow_End</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:endEvent id="EndEvent_Escalated" name="Processo Escalonado">
      <bpmn:incoming>Flow_End_Escalation</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:sequenceFlow id="Flow_End" sourceRef="Activity_Main" targetRef="EndEvent_1" />
    <bpmn:sequenceFlow id="Flow_End_Escalation" sourceRef="Activity_Escalation" targetRef="EndEvent_Escalated" />
  </bpmn:process>
  
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="thunderstruck-process">
      <bpmndi:BPMNShape id="_BPMNShape_StartEvent_2" bpmnElement="StartEvent_1">
        <dc:Bounds x="179" y="99" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Activity_DMN_di" bpmnElement="Activity_DMN">
        <dc:Bounds x="270" y="77" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Activity_Main_di" bpmnElement="Activity_Main">
        <dc:Bounds x="430" y="77" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Activity_Escalation_di" bpmnElement="Activity_Escalation">
        <dc:Bounds x="580" y="200" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_1">
        <dc:Bounds x="582" y="99" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_Escalated_di" bpmnElement="EndEvent_Escalated">
        <dc:Bounds x="732" y="222" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Event_SLA_Timer_di" bpmnElement="Event_SLA_Timer">
        <dc:Bounds x="482" y="139" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="215" y="117" />
        <di:waypoint x="270" y="117" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_To_Main_di" bpmnElement="Flow_To_Main">
        <di:waypoint x="370" y="117" />
        <di:waypoint x="430" y="117" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_End_di" bpmnElement="Flow_End">
        <di:waypoint x="530" y="117" />
        <di:waypoint x="582" y="117" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Escalation_di" bpmnElement="Flow_Escalation">
        <di:waypoint x="500" y="175" />
        <di:waypoint x="500" y="240" />
        <di:waypoint x="580" y="240" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_End_Escalation_di" bpmnElement="Flow_End_Escalation">
        <di:waypoint x="680" y="240" />
        <di:waypoint x="732" y="240" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>